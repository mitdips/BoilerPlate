{
  "expo": {
    "name": "boilerplate",
    "slug": "boilerplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["android", "ios", "web"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ips.boilerplate",
      "googleServicesFile": "./firebase/GoogleService-Info.plist",
      "userInterfaceStyle": "automatic",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "FirebaseStaticFramework": true
      }
    },
    "android": {
      "jsEngine": "hermes",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ips.boilerplate",
      "googleServicesFile": "./firebase/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "firebase": {
          "apiKey": "AIzaSyByrtM8-vEcqpljtSxIZEwo9g6tJRJd_kM",
          "authDomain": "boilerplate-55148.firebaseapp.com",
          "projectId": "boilerplate-55148",
          "storageBucket": "boilerplate-55148.firebasestorage.app",
          "messagingSenderId": "106330118056",
          "appId": "1:106330118056:web:676e63f84e9ec5cef4af9d",
          "measurementId": "G-X63Z4KCZJY"
        }
      }
    },
    "extra": {
      "ENABLE_USER_SCRIPT_SANDBOXING": false
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "boilerplate",
          "organization": "itpathsolutions-yt"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "ccacheEnabled": true,
            "useFrameworks": "static",
            "deploymentTarget": "15.1",
            "podfileProperties": {
              "use_modular_headers!": "true"
            }
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 23,
            "frescoVersion": "2.5.0",
            "kotlinVersion": "1.7.10",
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
